<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net8.0-windows</TargetFramework>
    <PublishSingleFile>true</PublishSingleFile>
    <SelfContained>false</SelfContained>
    <DebugType>embedded</DebugType>
    <PublishReadyToRun>true</PublishReadyToRun>
    <OutputType>WinExe</OutputType>
    <SupportUrl>https://www.unitgate.eu</SupportUrl>
    <ProductName>UnitGate</ProductName>
    <PublisherName>Allan Lie</PublisherName>
    <SuiteName>UnitGate</SuiteName>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>Images\icon.ico</ApplicationIcon>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="EPPlus" Version="7.5.2" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="Images\Lie-Consulting-Logo-V-2.png" />
    <Resource Include="Images\PayPal-Donate.png" />
    <Resource Include="Images\AllanLie.jpg" />
    <Resource Include="Images\icon.ico" />
  </ItemGroup>
</Project>